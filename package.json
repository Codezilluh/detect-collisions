{
	"name": "detect-collisions",
	"version": "2.3.0",
	"description": "[forked] Collision detection for circles, polygons, and points",
	"main": "source/index.js",
	"scripts": {
		"lint": "eslint ./source --fix",
		"compile": "babel ./source --out-dir ./es2015",
		"build": "rm -rf ./docs/* && esdoc && webpack"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Prozi/detect-collisions.git"
	},
	"keywords": [
		"Collision",
		"Separating Axis Theorem",
		"Bounding Volume Hierarchy",
		"SAT",
		"BVH",
		"Circle",
		"Polygon",
		"Line",
		"Shape",
		"Separating",
		"Axis",
		"Theorem",
		"Bounding",
		"Volume",
		"Hierarchy"
	],
	"author": "Samuel Hodge",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Prozi/detect-collisions/issues"
	},
	"homepage": "https://github.com/Prozi/detect-collisions#readme",
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-loader": "^7.1.4",
		"babel-preset-es2015": "^6.24.1",
		"chalk": "^2.4.1",
		"esdoc": "^1.1.0",
		"esdoc-standard-plugin": "^1.0.0",
		"html-webpack-plugin": "^3.2.0",
		"webpack": "^4.9.1",
		"webpack-command": "^0.2.0"
	},
	"dependencies": {},
	"editions": [
		{
			"description": "Source + ESNext + Import",
			"entry": "index.js",
			"directory": "source",
			"syntaxes": [
				"javascript",
				"esnext",
				"require",
				"arrows",
				"getset",
				"const",
				"let",
				"classes",
				"defaults",
				"spread",
				"rest",
				"destructuring",
				"template strings"
			]
		},
		{
			"description": "Compiled + ES2015 + Require",
			"entry": "index.js",
			"directory": "es2015",
			"syntaxes": [
				"javascript",
				"es2015",
				"require"
			]
		}
	]
}
